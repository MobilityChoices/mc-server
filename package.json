{
  "name": "mc-server",
  "version": "0.0.0",
  "description": "Mobility Choices Backend",
  "main": "src/main.js",
  "scripts": {
    "cover": "istanbul cover _mocha --include-all-sources -x 'src/main.js' -x 'scripts/*.js' -- 'test/**/*.js'",
    "develop": "nodemon .",
    "lab": "lab -t 100 --coverage-path 'src'",
    "test-cov-html": "lab -r html --coverage-path 'src' -o coverage.html -a chai -P 'lab' -L",
    "lint": "eslint src/",
    "test": "mocha 'test/**/*.js'"
  },
  "engines": {
    "node": "6.9.0"
  },
  "license": "ISC",
  "keywords": [],
  "author": "",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MobilityChoices/mc-server.git"
  },
  "bugs": {
    "url": "https://github.com/MobilityChoices/mc-server/issues"
  },
  "homepage": "https://github.com/MobilityChoices/mc-server#readme",
  "dependencies": {
    "axios": "0.15.2",
    "hapi": "15.2.0",
    "joi": "9.2.0",
    "jsonwebtoken": "7.1.9",
    "qs": "6.3.0"
  },
  "devDependencies": {
    "@types/axios": "0.9.33",
    "@types/chai": "3.4.34",
    "@types/hapi": "13.0.36",
    "@types/joi": "9.0.32",
    "@types/jsonwebtoken": "7.1.33",
    "@types/mocha": "2.2.32",
    "@types/qs": "6.2.30",
    "@types/sinon": "1.16.31",
    "chai": "3.5.0",
    "eslint": "3.8.1",
    "istanbul": "0.4.5",
    "lab": "11.1.0",
    "mocha": "3.1.2",
    "nodemon": "1.11.0",
    "sinon": "1.17.6"
  }
